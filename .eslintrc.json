{
    
    "env": {
        "browser": true,
        "es6": true
    },
    "extends": ["eslint:recommended"],
    "globals": {
        "jQuery": true,
        "$": true,
        "GoldenLayout": true
    },
    "ignorePatterns": [
        "webapp_release/libs/**/*.js",
        "tests/**/*.js"
    ],
    "parserOptions": {
        "sourceType": "module"
    },
    "rules": {
        "no-prototype-builtins": 0,
        "no-unused-vars": ["error", { "vars": "all", "args": "none", "ignoreRestSiblings": false }],
        "no-empty": ["error", { "allowEmptyCatch": true }]
    }
}